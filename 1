class operator
{
    public static void main(String args[])
    {
        int a=10;
        int b=20;
        int x,y;
        x=a+++b--;
        y=b--+a++;
        System.out.println(a);
        System.out.println(b);
        System.out.println(x);
        System.out.println(y);
    }
}
